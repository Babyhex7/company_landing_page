{
  "kind": "collectionType",
  "collectionName": "team_members",
  "info": {
    "singularName": "team-member",
    "pluralName": "team-members",
    "displayName": "Team Member",
    "description": "Anggota tim perusahaan"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "maxLength": 255
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "position": {
      "type": "string",
      "required": true
    },
    "bio": {
      "type": "text"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images"]
    },
    "email": {
      "type": "email"
    },
    "social_linkedin": {
      "type": "string"
    },
    "social_twitter": {
      "type": "string"
    },
    "social_instagram": {
      "type": "string"
    },
    "social_github": {
      "type": "string"
    },
    "order": {
      "type": "integer",
      "default": 0
    },
    "featured": {
      "type": "boolean",
      "default": false
    }
  }
}
